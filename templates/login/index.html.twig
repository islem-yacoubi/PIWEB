</head>
<body style="background-color: #905E96;">
<nav class="navbar navbar-inverse">
    <div class="container-fluid">
        <div class="navbar-header">
            <a class="navbar-brand" href="#">Artycamp</a>
        </div>
        <ul class="nav navbar-nav">
            <li class="active"><a href={{ path('app_home') }}>Home</a></li>
        </ul>
    </div>
</nav>
<form><center>
        {% block body %}
        {% if error %}
            <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
        {% endif %}

        <form action="{{ path('app_login') }}" method="post">
            <label for="username">Email:</label>
            <input type="email" id="form2Example1"  class="form-control" name="_username" value="{{ last_username }}">

            <label for="password">Password:</label>
            <input type="password" id="form2Example2" class="form-control" name="_password">

            {# If you want to control the URL the user is redirected to on success
        <input type="hidden" name="_target_path" value="/account"> #}
            <div class="col">
                <!-- Simple link -->
                <a href={{ path('app_password') }}>Forgot password?</a>
            </div>
            <button type="submit">login</button>
            <div class="col">
                <!-- Simple link -->
                <a href={{ path('app_user_new') }}>Create account?</a>
            </div>
        </form>
{% endblock %}